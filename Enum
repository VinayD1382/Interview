enum status{PENDING, WAITING, CONFIRMED };
class Order{
    status sta;
    Order(status sta){
        this.sta = sta;
    }
   public String toString(){
       return "Ordered " +  sta;
   }
    }
    public class Main{
    public static void main(String[] args){
        Order obj = new Order(status.PENDING);
        System.out.println(obj);
    }
    }
